class Solution {
    public int mySqrt(int x) {
        int low = 0, high = x, ans = 1;
        while(low <= high) {
            int mid = (low + high) / 2;
            long square = (long) mid * mid;
            if(square == x) {
                ans = mid;
                break;
            }
            else if(square < x) {
                ans = mid;
                low = mid + 1;
            }
            else high = mid - 1;
        }
        return ans;
    }
}
